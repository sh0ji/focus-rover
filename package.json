{
  "name": "focus-rover",
  "version": "2.0.3",
  "description": "Set a roving focus on a group of elements.",
  "keywords": [
    "a11y",
    "accessibility",
    "focus",
    "roving",
    "tabindex"
  ],
  "homepage": "https://github.com/sh0ji/focus-rover#readme",
  "bugs": {
    "url": "https://github.com/sh0ji/focus-rover/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh0ji/focus-rover.git"
  },
  "license": "MIT",
  "author": "Evan Yamanishi",
  "type": "module",
  "exports": "./dist/foo.modern.js",
  "main": "./dist/index.cjs",
  "unpkg": "./dist/index.umd.js",
  "module": "./dist/index.module.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "microbundle",
    "lint": "eslint '**/*.ts' --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "release": "standard-version --sign",
    "test": "nyc ava"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "helpers": [
      "**/helpers/**/*"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "instrument": true,
    "reporter": [
      "text",
      "html"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "ava": "^2.4.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "microbundle": "^0.13.3",
    "nyc": "^14.1.1",
    "standard-version": "^7.0.1",
    "typescript": "^3.7.4"
  }
}
